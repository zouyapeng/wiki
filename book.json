{
  "title": "Gitbook As Wiki",
  "description": "",
  "author": "Zouyapeng",
  "output.name": "site",
  "language": "zh-hans",
  "plugins": [
    "-livereload",
    "-lunr",
    "-search",
    "search-plus",
    "sectionx",
    "prism",
    "-highlight",
    "splitter",
    "book-summary-scroll-position-saver",
    "toggle-chapters",
    "spoiler",
    "github",
    "advanced-emoji",
    "-terminal",
    "donate",
    "-local-video",
    "copy-code-button",
    "-alerts"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/zouyapeng"
    },
    "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
    },
    "theme-default": {
         "showLevel": true
    },
    "donate": {
        "alipay": "https://raw.githubusercontent.com/zouyapeng/Image/master/alipay.png",
        "title": "",
        "button": "赏",
        "alipayText": "支付宝打赏"
    },
    "anchor-navigation-ex": {
            "isRewritePageTitle": true,
            "isShowTocTitleIcon": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        }
  }
}